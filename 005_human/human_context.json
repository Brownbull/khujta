    "iteration_templates": {
      "example_photo_text_extraction_app": {
        "iteration_1": {
          "focus": "extract_text_from_single_image",
          "deliverables": ["basic_ocr_function", "console_output"],
          "concepts_introduced": ["image_processing", "ocr_library"],
          "human_learning": "understand_core_extraction"
        },
        "iteration_2": {
          "focus": "process_multiple_images",
          "deliverables": ["batch_processing", "basic_error_handling"],
          "concepts_introduced": ["file_iteration", "exception_handling"],
          "human_learning": "understand_scaling"
        },
        "iteration_3": {
          "focus": "extract_meaningful_keywords",
          "deliverables": ["nlp_integration", "keyword_extraction"],
          "concepts_introduced": ["text_processing", "relevance_filtering"],
          "human_learning": "understand_intelligence_layer"
        },
        "iteration_4": {
          "focus": "structure_and_store_data",
          "deliverables": ["dataframe_creation", "data_persistence"],
          "concepts_introduced": ["data_structures", "file_formats"],
          "human_learning": "understand_data_organization"
        },
        "iteration_5": {
          "focus": "generate_reports",
          "deliverables": ["excel_export", "summary_statistics"],
          "concepts_introduced": ["reporting_libraries", "data_visualization"],
          "human_learning": "understand_output_generation"
        },
        "iteration_6": {
          "focus": "create_user_interface",
          "deliverables": ["gui_or_web_interface", "user_feedback"],
          "concepts_introduced": ["ui_frameworks", "event_handling"],
          "human_learning": "understand_user_interaction"
        }
      }
    },
    
    "ai_behavior_guidelines": {
      "when_building": {
        "always": [
          "start_with_working_minimal_example",
          "explain_before_implementing",
          "provide_runnable_code_each_iteration",
          "highlight_what_changed_from_previous",
          "suggest_next_logical_step"
        ],
        "avoid": [
          "jumping_to_complex_solution",
          "introducing_multiple_paradigms_simultaneously",
          "assuming_prior_knowledge",
          "skipping_intermediate_steps"
        ]
      },
      "when_explaining": {
        "structure": {
          "opening": "what_we_are_building_and_why",
          "middle": "step_by_step_implementation",
          "closing": "what_we_achieved_and_next_steps"
        },
        "techniques": [
          "use_analogies_to_familiar_concepts",
          "provide_visual_representations_when_helpful",
          "include_common_pitfalls_and_solutions",
          "reference_previous_iterations"
        ]
      }
    },
    
    "progress_tracking": {
