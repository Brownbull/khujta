        ],
        "complexity_indicators": [
          "lines_of_code",
          "number_of_files",
          "external_dependencies",
          "abstraction_levels"
        ]
      },
      "checkpoints": {
        "review_triggers": [
          "after_new_major_concept",
          "before_architectural_change",
          "when_errors_encountered",
          "at_natural_pause_points"
        ]
      }
